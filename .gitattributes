# GNS3 project files
*.gns3 project filter=lfs diff=lfs merge=lfs -text
*.gns3project filter=lfs diff=lfs merge=lfs -text
*.gns3project.backup filter=lfs diff=lfs merge=lfs -text

# VM disk images
*.qcow2 filter=lfs diff=lfs merge=lfs -text
*.vmdk filter=lfs diff=lfs merge=lfs -text
*.vdi filter=lfs diff=lfs merge=lfs -text
*.iso filter=lfs diff=lfs merge=lfs -text

# Common archives
*.zip filter=lfs diff=lfs merge=lfs -text
*.tar.gz filter=lfs diff=lfs merge=lfs -text
*.7z filter=lfs diff=lfs merge=lfs -text
